[
  {
    "trigger": {
      "url-filter": ".*",
      "unless-domain": ["*github.com", "*scala-lang.org", "*oracle.com", "developer.apple.com", "192.168.100.1", "*objectivepi.com", "orc.csres.utexas.edu", "login.utexas.edu", "utdirect.utexas.edu"]
    },
    "action": {
      "type": "block"
    }
  },
  {
    "trigger": {
      "if-top-url": ["file:///.*", "file://localhost/.*"]
    },
    "action": {
      "type": "ignore-previous-rules"
    }
  }
]
